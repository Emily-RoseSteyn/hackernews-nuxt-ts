{
  "name": "nuxt-ts-hn",
  "description": "Nuxt Typescript Hacker News",
  "version": "1.0.0",
  "author": "Evan You <yyx990803@gmail.com>",
  "contributors": [
    {
      "name": "Sebastien Chopin (@Atinux)"
    },
    {
      "name": "Alexandre Chopin (@alexchopin)"
    },
    {
      "name": "Pooya Parsa (@pi0)"
    },
    {
      "name": "HG(@husayt)"
    }
  ],
  "private": true,
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "scripts": {
    "dev": "nuxt-ts dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "dev-spa": "nuxt dev --spa",
    "build-spa": "nuxt build --spa",
    "start-spa": "nuxt start --spa",
    "tsc": "tsc --showConfig",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "testc": "tsc -p tsconfig.json",
    "lint": "tslint --project .",
    "lintfix": "tslint --fix --project ."
  },
  "now": {
    "alias": "hn.nuxtjs.org"
  },
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.6.0",
    "@nuxtjs/component-cache": "1.1.5",
    "@nuxtjs/pwa": "3.0.0-beta.16",
    "apicache": "1.5.2",
    "express": "4.17.1",
    "nuxt-property-decorator": "2.4.0",
    "nuxt": "2.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@nuxt/types": "^0.2.6",
    "@nuxt/typescript-build": "^0.2.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^10.14.17",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "latest",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "lodash-es": "4.17.11",
    "prettier": "^1.18.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^6.0.4",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.2",
    "vue-jest": "^3.0.4"
  },
  "jest": {
    "moduleFileExtensions": ["js", "ts", "json", "vue"],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "snapshotSerializers": ["jest-serializer-vue"]
  }
}
